{
  "name": "@x402apis/client",
  "version": "1.1.4",
  "description": "Universal Client SDK and CLI for the x402 API Router",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": {
        "node": "./dist/index.js",
        "browser": "./dist/browser.js",
        "default": "./dist/index.js"
      },
      "require": {
        "node": "./dist/index.js",
        "browser": "./dist/browser.js",
        "default": "./dist/index.js"
      }
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "x402-router": "./dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/cli/index.ts",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "x402",
    "api",
    "router",
    "client",
    "cli",
    "solana",
    "micropayments"
  ],
  "author": "x402apis",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/x402apis/x402-router-client"
  },
  "dependencies": {
    "@solana/spl-token": "^0.4.14",
    "@solana/web3.js": "^1.87.0",
    "@x402apis/protocol": "^1.0.2",
    "commander": "^11.1.0",
    "inquirer": "^8.2.5",
    "node-fetch": "^2.7.0",
    "open": "^9.1.0",
    "qrcode": "^1.5.3"
  },
  "devDependencies": {
    "@types/inquirer": "^8.2.0",
    "@types/node": "^20.0.0",
    "@types/node-fetch": "^2.6.9",
    "@types/qrcode": "^1.5.5",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "prettier": "^3.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.0"
  },
  "files": [
    "dist",
    "README.md"
  ]
}
